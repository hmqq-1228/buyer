.develop_box{
  width: 100%;
  height: 100%;
  position: absolute;
  background: #F2F2F2;
}
.section{
  width: 100%;
  height: 80rpx;
  border: 1px solid gainsboro;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 80rpx 0 0;
  position: relative;
}
.section__title{
  width: 492rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #A9A2A4;
  padding-left: 10rpx;
}
.section_img{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 15rpx;
  right: 5rpx;
}
.end_box{
  width: 100%;
  display: flex;
  position: relative;
}
.person_looking{
  width: 50%;
  height: 80rpx;
  border: 1px solid gainsboro;
  background-color: #ccc;
  display: flex;
  font-size: 28rpx;
  line-height: 70rpx;
  padding-left: 15rpx;
}
.shenqing_word{
  width: 26%;
  font-size: 28rpx;
  background-color: #fff;
  padding-left: 15rpx;
  height: 80rpx;
  border-top: 1px solid gainsboro;
  border-bottom: 1px solid gainsboro;
  line-height: 79rpx;
}
.shenqing{
  width: 24%;
  height: 80rpx;
  border-top: 1px solid gainsboro;
  border-bottom: 1px solid gainsboro;
  background-color: #fff;
  display: flex;
  font-size: 28rpx;
  line-height: 70rpx;
  padding-left: 15rpx;
}
.placeholder{
  color: #AAA;
}
.jinru{
  position: absolute;
  top: 15rpx;
  right: 5rpx;
  width: 50rpx;
  height: 50rpx;
}
.picker{
  height: 80rpx;
  line-height: 70rpx;
  text-align:right;
}
picker{
  font-size: 28rpx;
  width: 70%;
}
.select{
  width: 50%;
  max-height: 400rpx;
  background: #FFF; 
  position: absolute;
  top: 80rpx;
  right: 0;
  display: flex;
  flex-direction: column;
  overflow: scroll;
}
.btn{
  width: 100%;
  position:absolute;
  bottom: 0;

}