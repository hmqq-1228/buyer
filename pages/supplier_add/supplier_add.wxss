.supplier_box{
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
text{
  font-size: 14px;
  color: #808080;
  flex-shrink: 0;
  margin-left: 10rpx;
}
input{
  width: 100%;
  margin-left: 15rpx;
}
.supplier_word{
  margin-right:-4rpx;
  color:red;
}
.exclamation{
  width: 40rpx;
  height: 40rpx;
  /* padding-left: 20rpx; */
}
.hidden{
  display: none;
}
.show{
  display: block;
}
.add_box{
  width: 100%;
  display: flex;
  align-items: center;
  border: 1px solid gainsboro;
  background-color: #Ffff;
  height: 80rpx;
}
.add_ress{
   width: 100%;
  height: 140rpx;
  border: 1px solid gainsboro;
  background-color: #Ffff;
}
.add_id{
  font-size: 14px;
  color: #808080;
  flex-shrink: 0;
  margin: 10rpx 0 0 10rpx;
}
.add_text{
  width: 100%;
  height: 80rpx;
}
.use1{
  width: 100%;
  height: 100rpx;
  border: 1px solid gainsboro;
  background-color: #Ffff;
}
.use2{
  width: 100%;
  height: 92rpx;
  background-color: #Ffff;
}
.add_space{
  width: 100%;
  height: 40rpx;
  background: #F2F2F2;
}
